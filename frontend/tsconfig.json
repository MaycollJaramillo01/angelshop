{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["vite/client"],
    "baseUrl": "./",
    "paths": {
      "reactstrap": ["./src/vendor/reactstrap"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
